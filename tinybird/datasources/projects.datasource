SCHEMA >
    `id` String,
    `userId` String,
    `name` String,
    `domain` String,
    `createdAt` DateTime64(3),
    `updatedAt` DateTime64(3),
    `status` Enum('active', 'paused', 'deleted'),
    `settings` String,
    `metadata` String

ENGINE "ReplacingMergeTree"
ENGINE_VER "updatedAt"
ENGINE_SORTING_KEY "id, userId, createdAt"
