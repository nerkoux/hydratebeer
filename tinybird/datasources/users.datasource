SCHEMA >
    `id` String,
    `email` String,
    `name` String,
    `createdAt` DateTime64(3),
    `updatedAt` DateTime64(3),
    `plan` Enum('free', 'pro', 'enterprise'),
    `stripeCustomerId` String,
    `metadata` String

ENGINE "ReplacingMergeTree"
ENGINE_VER "updatedAt"
ENGINE_SORTING_KEY "id, email"
